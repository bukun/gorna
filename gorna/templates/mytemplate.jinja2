{% extends "layout.jinja2" %}

{% block content %}
    <div class="content">


        <div class="starter-template">

            <div class="panel panel-default">
            <div class="panel-head">
                <h1>人地系统数据库</h1>
            </div>
                <div class="panel-body">
                    {% for all_file in all_files %}
                        <h4>{{ all_file[0] }}</h4>

                        <div class="row">

                            <div class="panel panel-default">
                                <div class="panel-body">
                                    {% for xlsx in all_file[1] %}
                                        <a href="/datable/{{ xlsx[1] }}" class="btn btn-primary">{{ xlsx[0] }}</a>
                                    {% endfor %}
                                </div>
                            </div>
                        </div>
                    {% endfor %}
                </div>
            </div>
        </div>



    </div>
{% endblock content %}
